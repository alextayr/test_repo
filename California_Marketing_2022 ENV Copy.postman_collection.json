	"item": [
		{
			"name": "Post a new address",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"pm.test(\"Response time is less than 10000ms\", function () {",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);",
							"});",
							"pm.test(\"Response size is less than 600B\", function () {",
							"    pm.expect(pm.response.responseSize).to.be.below(600);",
							"});",
							"pm.test(\"Successful POST request\", function () {",
							"    pm.expect(pm.response.code).to.be.oneOf([200]);",
							"});",
							"pm.test(\"Header is correct\", function () {",
							"    pm.response.to.have.header(\"Content-Type\", \"application/json;charset=utf-8\");",
							"    pm.response.to.have.header(\"Content-Length\", \"45\");",
							"    pm.response.to.have.header(\"Connection\", \"keep-alive\");",
							"});",
							"",
							"var jsonData = pm.response.json();",
							"pm.environment.set(\"ID\", jsonData.id);",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.environment.unset(\"ID\");",
							"pm.environment.unset(\"Comp\");",
							"",
							"pm.environment.set(\"Comp\", \"Netflix-\" + new Date().getTime());",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
